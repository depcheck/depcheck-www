{
  "name": "depcheck-web",
  "description": "Depcheck web service",
  "repository": "https://github.com/lijunle/depcheck-web",
  "license": "MIT",
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  },
  "scripts": {
    "lint": "babel-node ./node_modules/gulp/bin/gulp.js lint",
    "depcheck": "depcheck --ignore-bin-package=false --specials=bin,eslint --ignores bootstrap",
    "build": "babel-node ./node_modules/gulp/bin/gulp.js build",
    "build-azure": "babel-node ./node_modules/gulp/bin/gulp.js build-azure",
    "test": "mocha --compilers js:babel/register --recursive ./test --timeout 5000",
    "test-coverage": "babel-node ./node_modules/.bin/isparta cover node_modules/mocha/bin/_mocha -- --recursive ./test --timeout 5000",
    "local": "babel-node ./src/index.js"
  },
  "dependencies": {
    "azure-storage": "^0.6.0",
    "body-parser": "^1.14.1",
    "bootstrap": "^3.3.5",
    "express": "^4.13.3",
    "express-react-views": "^0.9.0",
    "express-session": "^1.11.3",
    "locallydb": "0.0.9",
    "node-uuid": "^1.4.3",
    "path-to-regexp": "^1.2.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "request": "^2.65.0",
    "serve-favicon": "^2.3.0",
    "session-file-store": "0.0.21",
    "winston": "^1.1.0",
    "winston-azuretable": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "cheerio": "^0.19.0",
    "codecov.io": "^0.1.6",
    "del": "^2.0.2",
    "depcheck-es6": "^0.5.8",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-eslint": "^1.0.0",
    "isparta": "^3.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "proxyquire": "^1.7.3",
    "should": "^7.1.1",
    "supertest": "^1.1.0"
  }
}
